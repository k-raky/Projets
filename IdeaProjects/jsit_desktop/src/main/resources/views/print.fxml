<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.printController">
   <children>
      <BorderPane layoutX="2.0" prefHeight="492.0" prefWidth="583.0" style="-fx-background-color: white;">
         <top>
            <HBox alignment="CENTER" prefHeight="62.0" prefWidth="583.0" BorderPane.alignment="CENTER">
               <children>
                  <Text fx:id="titre" fill="#23c7c9" strokeType="OUTSIDE" strokeWidth="0.0">
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font>
                  </Text>
               </children>
            </HBox>
         </top>
         <bottom>
            <Button mnemonicParsing="false" onAction="#imprimer" text="Imprimer" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </BorderPane.margin></Button>
         </bottom>
         <center>
            <ScrollPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <content>	
                  <VBox fx:id="listFilesContainer" alignment="CENTER" prefHeight="365.0" prefWidth="557.0" style="-fx-background-color: white;">
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                   </VBox>
               </content>
            </ScrollPane>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
